    <style>
        /* Start Global Variable */

:root {
    --main-color: #654135;
    --padding: 60px;
    --secondly-color: #8d6e63;
    --third-color: #704b3d;
    --main-transition: .3s;
}

/* End Global Variable */


/* Start Component */

/* End Component */


/* Start Global CSS */
* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

html {
    scroll-behavior: smooth;
}

body {
    font-family: "Open Sans", sans-serif;
    font-style: italic;
    letter-spacing: 1px;
    /*background-color: #f6f6f6;*/
    /* height: 2000px; */
    /* position: relative; */
}
[type="submit"],
[type="button"],
button {
    cursor: pointer;
}

a {
    text-decoration: none;
}

ul {
    list-style: none;
}

::selection {
    background-color: var(--main-color);
    color: white;
}

::-webkit-scrollbar {
    width: 5px;
}

::-webkit-scrollbar-track {
    background-color: #efefef;
}

::-webkit-scrollbar-thumb {
        border-radius: 0 0 5px 5px;
    background-image: linear-gradient(to bottom, var(--main-color), #daae68);
}


.main-title {
  font-weight: bold;
  font-size: 27px;
  border: 2px solid var(--main-color);
  width: fit-content;
  padding: 8px 20px;
  margin: 0 auto;
  margin: 0px auto 65px;
  border-radius: 6px;
  color: var(--main-color);
  position: relative;
  transition: var(--main-transition);
  z-index: 3;
}
.main-title:hover {
  transition-delay: 0.5s;
  color: white;
  border: 2px solid transparent;
}
.main-title::before,
.main-title::after {
  content: "";
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  background-color: var(--main-color);
  width: 17px;
  height: 17px;
  border-radius: 50%;
}

.main-title::before {
  left: -35px;
}
.main-title::after {
  right: -35px;
}

.main-title:hover::before {
  animation: moveLeft 0.5s linear forwards;
  z-index: -1;
}
.main-title:hover::after {
  animation: moveRight 0.5s linear forwards;
  z-index: -1;
}

 .main-title2 {
                background-color: white;
                width: 100%;
                overflow: hidden;
                border: none;
                text-align: center;
                padding: 15px;
        }
.main-title2.main-title {
    border: none;
        margin-bottom: 15px;
            
}

.sr {
    position: relative;
}
.sr:before {
content: "SR";
    position: absolute;
    right: 4px;
    bottom: 12px;
    font-size: 9px;
    color: #4caf50;
    font-weight: bold;

}

@keyframes moveLeft {
  50% {
    width: 17px;
    height: 17px;
    left: 0;
  }
  100% {
    border-radius: 6px;
    left: 0;
    width: 60%;
    height: 100%;
  }
}
@keyframes moveRight {
  50% {
    width: 17px;
    height: 17px;
    right: 0;
  }
  100% {
    border-radius: 6px;
    right: 0;
    width: 60%;
    height: 100%;
  }
}

.container {
    padding: 0 var(--padding);
    margin: 0 auto;
}

@media (min-width: 768px) {
    .container {
        width: 750px;
    }
}

@media (min-width: 992px) {
    .container {
        width: 950px;
    }
}

@media (min-width: 1200px) {
    .container {
        width: 1170px;
    }
}

/* End Global CSS */

/* Start Header */

header {
  padding: 30px 0;
  background-color: white;
  position: relative;
      box-shadow: 0 2px 15px #eee;
}

header .container {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

@media (max-width: 767px) {
  header .container {
    flex-direction: column;
    gap: 20px;
  }
}

header .container .logo {
  color: var(--main-color);
  font-size: 20px;
  display: flex;
}

header .container .logo a {
  color: var(--main-color);
  position: relative;
  top: 5px;
}
header .container .logo .coffee {
  font-size: 40px;
  margin-right: 15px;
}

header .container .links {
  display: flex;
  gap: 15px;
}
header .container .links li {
}
header .container .links li a,
header .container .links li [type="submit"] {
  height: 40px;
  color: var(--main-color);
  position: relative;
  overflow: hidden;
  display: flex;
  justify-self: center;
  align-items: center;
  padding: 0 10px;
  border: none;
  outline: none;
  background-color: transparent;
  cursor: pointer;
  /* min-width: 85px; */
}
header .container .links li [type="submit"]:hover {
    font-weight: bold;
}
header .container .links li a::before {
  content: "";
  position: absolute;
  bottom: 0px;
  left: -100%;
  width: 100%;
  height: 2px;
  background-color: var(--main-color);
  transition: var(--main-transition);
}
header .container .links li a:hover::before {
  left: 0;
}

/* header .container .links li:last-child:hover .mega, */
.open-mega {
  opacity: 1;
  z-index: 100;
  top: calc(100% + 2px);
}

header .container .mega {
  position: absolute;
  left: 0;
  padding: 30px 0;
  background-color: rgb(255, 254, 254);
  width: 100%;
  border-bottom: 3px solid var(--main-color);
  display: flex;
  justify-content: space-around;
  /* gap: 40px; */
  transition: top var(--main-transition), opacity var(--main-transition);
}
.close-mega {
  top: calc(100% + 50px);
  opacity: 0;
  z-index: -1;
}

@media (max-width: 400px) {
  header .container .mega {
    flex-direction: column;
  }
}
header .container .mega .image {
  margin: auto 0 auto 3px;
}
header .container .mega .image img {
  width: 450px;
  height: 250px;
      border-radius: 100px 0 100px 0;
  transition: var(--main-transition);
}

@media (max-width: 991px) {
  header .container .mega .image img {
    width: 250px;
  }
}
@media (max-width: 767px) {
  header .container .mega .image img {
    display: none;
  }
}

header .container .sub-links {
  margin: auto 40px;
  flex-grow: 1;
}
header .container .sub-links li:not(:last-of-type) {
  border-bottom: 1px solid #d4d4d4;
}
@media (max-width: 400px) {
  header .container .sub-links:first-of-type li:last-of-type {
    border-bottom: 1px solid #d4d4d4;
  }
}
header .container .sub-links li a {
  padding: 40px 10px;
  min-width: 180px;
  font-weight: bold;
}

header .container .sub-links li a:hover {
}

header .container .sub-links li i {
  color: #ffc107;
  margin-right: 7px;
  font-size: 25px;
}

header .container .sub-links li {
  position: relative;
}
                    
                   
li:empty {
display: none;
}
.underline:empty {
display: none;
}

.hide {
display: none;
}
.hide_emp {
    display: none;
}

                

                     

                    
                    

/* End Header */

.up {
  background-color: var(--main-color);
  color: white;
  padding: 5px;
  position: fixed;
  z-index: 100;
  bottom: 10px;
  right: -40px;
  border-radius: 5px;
  transform: rotate(180deg);
  transition: var(--main-transition);
  cursor: pointer;
}
.up.show {
  right: 10px;
  transform: rotate(0deg);
}
.up i {
}

/* Start Settings */

.settings {
  position: fixed;
  height: 100%;
  width: 225px;
  background-color: white;
  left: -225px;
  top: 0;
  z-index: 200;
  border: 2px solid #ccc;
  transition: 1s;
}

.settings .icon {
  position: absolute;
  right: -35px;
  top: 50%;
  transform: translateY(-59%);
  color: var(--main-color);
  background-color: #ffffff;
  padding: 5px 0;
  width: 33px;
  text-align: center;
  border-radius: 0 5px 5px 0;
  border: 2px solid #e4e4e4;
  border-left: none;
  cursor: pointer;
}
.settings .icon i {
  color: var(--main-color);
}
.left {
  left: 0;
}
.settings .image {
    border-radius: 50%;
    width: 125px;
    height: 125px;
    display: block;
    margin: 25px auto;
    text-align: center;
    overflow: hidden;
    position: relative;
    cursor: pointer;

}
.settings .image::before {
    content:"";
    transition: .7s;
    position: absolute;
    left: 50%;
    bottom: 5%;
    transform: translateX(-50%);
    width: 100%;
    height: 100%;
    z-index: 4;
}
.settings .image:hover::before {
    background-image: url(image1/add1.png);
    background-size: cover;
    width: 30%;
    height: 30%;

}
.settings .image::after {
    content:"";
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    transition: .7s;
    z-index: 3;
}
.settings .image:hover:after {
    
    background-color: #0000001f;
}
.settings .image img {
      border-radius: 50%;
  width: 125px;
    height: 125px;
}
.settings .names {
  display: flex;
  justify-content: center;
}
.settings h5 {
  margin: 0 7px 0px 0;
  font-size: 19px;
  font-weight: normal;
  text-transform: capitalize;
}
.settings .come {
    position: absolute;
    bottom: 0;
    width: 100%;

}
.settings .come li {
  border-bottom: 2px solid #d4d4d4;
  padding: 33px 0 10px 3px;
  color: #777;
  position: relative;
  cursor: pointer;
  overflow: hidden;
}
.settings .come li:last-of-type {
  border-bottom: none;
}
.settings .come li::before {
  content: "";
  position: absolute;
  left: -100%;
  bottom: 0;
  width: 100%;
  height: 2px;
  background-color: var(--main-color);
  transition: var(--main-transition);
}
.settings .come li:last-of-type:before {
  height: 0;
}
.settings .come li:hover::before {
  left: 0;
}
.settings .come li:hover {
  color: black;
  /* font-weight: bold; */
}
.settings .come .btn {
  display: flex;
  justify-content: space-evenly;
  align-items: center;
}
.settings .come li button {
  background-color: transparent;
  width: 40%;
  padding: 4px 0;
  font-size: 14px;
  border: none;
  color: white;
  font-weight: bold;
  border-radius: 5px;
  background-color: var(--main-color);
  opacity: 0.7;
  margin-top: 20px;
}
.settings .come li .open {
  opacity: 1;
}
.settings .come li .passive {
}
.settings .come li .rest {
  width: 88%;
  background-color: red;
}
.settings .come li .rest:active {
  box-shadow: 0 2px 15px #aaa;
}
/* End Settings */

/* Start Settings Extend */
.overlay {
  height: 100%;
  width: 100%;
  background-color: #0000003b;
  position: fixed;
  top: 0%;
  left: 0;
  z-index: 1000;
  transition: 1s;
}

.change {
  position: fixed;
  top: 50%;
  left: 50%;
  background-color: white;
  z-index: 1000;
  transform: translate(-50%, -50%);
  width: 250px;
  height: 250px;
  border-radius: 6px;
  border-top-left-radius: 0;
  border: 2px solid var(--main-color);
  transition: 1s;
  position: relative;
}
.change-password {
  height: 300px;
}
.change-username {
  height: 200px;
}
.change-avatar {
    height: 200px;
}

.change .rem {
  content: "X";
  width: 40px;
  height: 40px;
  background-color: var(--main-color);
  color: white;
  position: absolute;
  border-radius: 50%;
  right: -16px;
  top: -16px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-style: normal;
  font-weight: bold;
  font-size: 20px;
  cursor: pointer;
}

.hide {
  z-index: -1;
  opacity: 0;
}
.hide0,
.hide1,
.hide2,
.hide3,
.hide_admin{
  display: none;
}
.change .change-name {
  text-align: center;
}
.change .change-name h4 {
  color: var(--main-color);
  margin: 19px auto 27px 0;
}
.change .change-name input:not([type="submit"]) {
  padding: 8px 0;
  margin-bottom: 18px;
  border-radius: 6px;
  border: none;
  border-bottom: 2px solid var(--main-color);
  text-align: center;
  font-weight: bold;
  letter-spacing: 1px;
  color: var(--main-color);
}
.change .change-name input:not([type="submit"]):focus {
  outline: none;
}
.change .change-name [type="submit"] {
  margin-top: 19px;
  padding: 6px 30px;
  background-color: var(--main-color);
  color: white;
  border: none;
  border-radius: 6px;
  font-weight: bold;
}

.error {
  position: fixed;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  background-color: var(--main-color);
  box-shadow: 0 2px 15px #d4d4d4;
  color: white;
  width: 275px;
  text-align: center;
  padding: 24px;
  border-radius: 0 0 5px 5px;
  font-weight: bold;
  transition: 1s;
  z-index: 1000;
}
/* End Settings Extend */

.change_img {
        background: var(--main-color);
    color: white;
    padding: 10px;
    font-size: 15px;
    border-radius: 10px;
