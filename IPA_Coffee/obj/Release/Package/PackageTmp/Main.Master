}

#theprogress {
    /*background-color: aliceblue;*/
    width: 50px;
    height: 24px;
    text-align: center;
    /*filter: alpha(Opacity=100);*/
    /*opacity: 1;*/
    /*-noz-opacity: 1;*/
}

#modalprogress {
    position: absolute;
    top: 30%;
    left: 30%;
    margin: -40px 0 0 55px;
    color: white;
}

body > #modalprogress {
    position: fixed;
}
.loading {
  box-sizing: border-box;
  width: 50px;
  /* background-color: red; */
  height: 50px;
  border-radius: 50%;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  border: 4px solid #03a9f4;
  border-right-color: transparent;
  transform: rotate(0deg);
  animation: Circle 1.7s linear infinite;
}

.loading::before,
.loading::after {
  box-sizing: border-box;
  content: "";
  position: absolute;
  left: 0px;
  top: 0;
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.loading::before {
  transform: scale(2);
  border: 2px solid#ffc107;
  border-left-color: transparent;
}
.loading::after {
  transform: scale(3);
  border: 2px solid #795548;
  border-bottom-color: transparent;
}
.load {
  position: fixed;
  font-size: 50px;
  width: 100%;
  text-align: center;
  left: 0;
  top: 25%;
  margin-left: 37px;
  color: white;
  /* transform: translateY(-50%); */
}

@keyframes Circle {
  100% {
    transform: rotate(360deg);
  }
}


